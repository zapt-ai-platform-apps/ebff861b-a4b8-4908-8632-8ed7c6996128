{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": "drizzle/schema.js"
    }
  },
  "crons": [
    {
      "path": "/api/cron/crosspost",
      "schedule": "*/5 * * * *"
    }
  ]
}